<% content_for(:page_title, t('.title')) %>
<% content_for :page_description, t('.'+@party.org_type+ '_description') %>
<%= ds_form_with(model: @party, url: @post_path, method: :post) do |f| %>
  <%= f.ds_text_field(method: :contact_firstname) %>
  <%= f.ds_text_field(method: :contact_surname) %>
  <%= f.ds_text_field(method: :job_title) %>
  <%= ds_address_search(address_summary: @address_summary, search_results: @search_results , address: @address_detail, 
      show_address_detail: @show_manual_address, readonly: @address_read_only, address_list: @address_list ) %>
  <%= f.ds_text_field(method: :contact_email) %>
  <%= f.ds_text_field(method: :contact_tel_no) %>
<% end %>
