<% content_for(:page_title, t('.title')) %>
<%= ds_form_with(model: @tax, url: @post_path, method: :post) do |f| %>
  <%= f.ds_currency(method: :npv,
        interpolations: {link: ds_link(url: external_link_url('TAX_ON_RENT'), name: t('activemodel.hints.returns/lbtt/tax.npv_link'), target: '_blank') }) %>  
  <% if @lbtt_return.linked_ind == 'Y' %>
    <%= f.ds_currency(method: :linked_npv) %>
  <% end %>
<% end %>
