<% content_for(:page_title, t('.title')) %>
<%= ds_section_title(section_title: t('.section_title')) %>
<%= ds_form_with(model: @property, url: @post_path, method: :post) do |f| %>
  <%= f.ds_select(method: :lau_code, select_options: @property.list_ref_data(:lau_code)) %>
  <%= ds_field_set( legend: t('.title_code'), alignment: :vertical) do %>
    <%= f.ds_select(method: :title_code, select_options: @property.list_ref_data(:title_code), show_label: false, width: 5) %>
    <%= f.ds_text_field(method: :title_number, show_label: false) %>
  <% end %>
  <%= ds_field_set( legend: t('.parent_title_code'), hint: t('.parent_title_hint'), alignment: :vertical) do %>
    <%= f.ds_select(method: :parent_title_code, select_options: @property.list_ref_data(:parent_title_code),
                         show_label: false, width: 5) %>
    <%= f.ds_text_field(method: :parent_title_number, show_label: false) %>
  <% end %>
<% end %>
