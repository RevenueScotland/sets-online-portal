<% content_for :hide_back_link, :lbtt_property_address_url %>
<% content_for :page_title, t('.title') %>
<% content_for :page_subtitle, t('.subtitle') %>  

<% content_for :page_view, render_page_view(__FILE__) %>
<%= form_for @property, url: @post_path, method: :post, local: true do |f| %>
<%= form_errors_for [@property, @address_summary, @address_detail] %>
  <h3> Property Address </h3>
  <%= render "/layouts/address" %>
  <%= f.button 'next', { :name => 'submitted' }  %>
<% end %>
