<% content_for :page_title, t('.title') %>
<% content_for :page_view, render_page_view(__FILE__) %>
<%= form_for @property, url: @post_path, method: :post, local: true do |f| %>
  <%= form_errors_for @property %>
  <%= f.collection_radio_buttons_fields(:ads_due_ind, @property.list_ref_data(:ads_due_ind),
                                        text_link: { t('.ads_due_ind_link') =>
                                                    link_to(t('.ads_due_ind_link'), "#{Rails.configuration.x.external_links.lbtt10001_ads}", target: '_blank') }) %> 
  <%= f.button 'next', { :name => 'submitted' }  %>
<% end %>