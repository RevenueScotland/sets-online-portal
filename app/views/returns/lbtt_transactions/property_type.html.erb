<% content_for :page_title, t('.title') %>
<% content_for :page_view, render_page_view(__FILE__) %>
<%= form_for @lbtt_return, url: returns_lbtt_property_type_path, :method => 'post', local: true do |f| %>
   <%= form_errors_for @lbtt_return %>
   <%= f.collection_radio_buttons_fields(:property_type, @lbtt_return.list_ref_data(:property_type), :code, :value) %>
<%= f.button 'next', { :name => 'submitted' }  %>
<% end %>
