<% content_for(:page_title, t('.title')) %>
<%= ds_form_with(model: @lbtt_return, url: @post_path, method: :post) do |f| %>
  <%= rs_conditional_visibility(visible_value: "Y") do |c| %>
    <% c.with_visibility_control_radio_group(builder: f.builder, method: :business_ind, options_list: @lbtt_return.list_ref_data(:business_ind), alignment: :horizontal) %>
    <%= f.ds_checkbox_group(method: :sale_include_option, options_list: @lbtt_return.list_ref_data(:sale_include_option)) %>
  <% end %>
<% end %>
