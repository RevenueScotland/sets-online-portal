<% content_for :page_title, t('.title') %>
<% content_for :page_subtitle, t('.subtitle') %>
<% content_for :page_view, render_page_view(__FILE__) %>
<%= form_for @lbtt_return, url: @post_path, :method => 'post', local: true do |f| %>
   <%= form_errors_for @lbtt_return %>
   <% f.current_browser = application_browser %>
   <%= f.date_field :effective_date %>
   <%= f.date_field :relevant_date %>
   <%= f.date_field :contract_date %>
   <% unless @lbtt_return.flbt_type == 'CONVEY' %>
      <%= f.date_field :lease_start_date %>
      <%= f.date_field :lease_end_date %>
   <% end %>
   <%= f.button 'next', { :name => 'submitted' }  %>
<% end %>