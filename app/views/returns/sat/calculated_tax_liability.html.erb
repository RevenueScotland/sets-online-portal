<% content_for(:page_title, t('.title')) %>
<%= ds_section_sub_title(section_sub_title: t('.sub_title')) %>
  <%= ds_form_with(model: @sat_return, url: @post_path, method: :post, button_action: :calculate_return) do |f| %>
      <%= f.ds_currency(method: :total_tax_due, readonly: true) %>
      <%= f.ds_currency(method: :total_credit, readonly: true) %>
      <%= f.ds_currency(method: :tax_payable, readonly: true) %>
  <% end %>
