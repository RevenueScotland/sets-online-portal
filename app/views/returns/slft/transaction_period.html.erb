<% content_for(:page_title, t('.title')) %>
<%= ds_form_with(model: @slft_return, url: @post_path, method: :post) do |f| %>
  <%= f.ds_select(method: :year, select_options: @slft_return.list_ref_data(:year)) %>
  <%= f.ds_radio_group(method: :fape_period, options_list: @slft_return.list_ref_data(:fape_period)) %>
<% end %>
