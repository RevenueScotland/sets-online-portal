<% content_for(:page_title, t('.title')) %>
<%= ds_form_with(model: @slft_return, url: @post_path, method: :post) do |f| %>
  <%= rs_conditional_visibility(visible_value: "Y") do |c| %>
    <% c.with_visibility_control_radio_group(builder: f.builder, method: :non_disposal_delete_ind, options_list: @slft_return.list_ref_data(:non_disposal_delete_ind), alignment: :horizontal) %>
    <%= f.ds_text_area(method: :non_disposal_delete_text) %>
  <% end %>
<% end %>
