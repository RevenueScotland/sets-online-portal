<% content_for(:page_title, t('.title')) %>
<%= ds_form_with(model: @slft_return, url: @post_path, method: :post) do |f| %>
  <%= rs_conditional_visibility(visible_value: "Y") do |c| %>
    <% c.with_visibility_control_radio_group(builder: f.builder, method: :repayment_yes_no, options_list: @slft_return.list_ref_data(:repayment_yes_no), alignment: :horizontal) %>
    <%= f.ds_currency(method: :amount_claimed) %>
  <% end %>
<% end %>
