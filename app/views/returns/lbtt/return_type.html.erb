<% content_for(:page_title, t('.title')) %>
<%# use the request path to remove the new parameter %>
<% content_for(:navigation_link, request.path) %>
<%= ds_form_with(model: @lbtt_return, url: @post_path, method: :post, action_name: :confirm) do |f| %>
  <%= f.ds_hidden_field(method: :current_flbt_type, value: @lbtt_return.flbt_type) %>
  <%= f.ds_radio_group(method: :flbt_type, options_list: @lbtt_return.list_ref_data(:flbt_type) ) %>
<% end %>
