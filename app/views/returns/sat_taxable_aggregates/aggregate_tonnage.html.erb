<% content_for(:page_title, t('.title', site: @taxable_aggregate.site_name)) %>
<%= ds_section_title(section_title: t('.subtitle')) %>
<%= ds_form_with(model: @taxable_aggregate, url: @post_path, method: :post) do |f| %>
  <%= f.ds_text_field(method: :exploited_tonnage, width:20) %>
  <%= f.ds_text_field(method: :water_tonnage, width:20) %>
  <%= f.ds_radio_group(method: :mixed_ind, options_list: @taxable_aggregate.list_ref_data(:mixed_ind),
                       alignment: :horizontal) %>
<% end %>
