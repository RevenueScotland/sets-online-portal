<% content_for(:page_title, t('.title')) %>
<%= ds_section_title(section_title: t('.section_title')) %>
<%= ds_form_with(model: @agent, url: returns_lbtt_agent_address_path, method: :post, action_name: :confirm, autocomplete: true, button_action: (@show_manual_address ? :continue : :none)) do |f| %>
  <%= ds_address_search(address_summary: @address_summary, search_results: @search_results , address: @address_detail, 
      show_address_detail: @show_manual_address, readonly: @address_read_only) %>
  <%= f.ds_hidden_field(:party_id) %>
<% end %>
