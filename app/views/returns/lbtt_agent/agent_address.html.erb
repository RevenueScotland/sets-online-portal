<% content_for :page_view, render_page_view(__FILE__) %>
<%= form_for @agent, url: returns_lbtt_agent_address_path, method: :post, html:{autocomplete: "on"},  local: true do |f| %>
  <% content_for :page_title, t('.title') %>
  <% content_for :page_subtitle, t('.subtitle') %>
  <%= form_errors_for [@agent, @address_summary, @address_detail] %>
  <%= render "/layouts/address" %>
  <%= f.hidden_field :party_id %>
  <%= f.button %>
<% end %>
