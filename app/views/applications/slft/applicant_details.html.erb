<% content_for :page_view, render_page_view(__FILE__) %>
<% content_for :page_title, t('.title') %>
<% content_for :page_description, t('.description') %>
<%= form_for @landfill_operator, url: @post_path, method: :post, local: true do |f| %>
  <%= form_errors_for @landfill_operator %>
  <%= f.text_field :organisation_name%>
  <%= f.text_field :slft_registration_number%>
  <%= f.text_field :telephone_number %>
  <%= f.text_field :email_address %>
  <%= f.button %>
<% end %>
