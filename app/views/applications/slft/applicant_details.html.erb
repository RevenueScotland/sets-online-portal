<% content_for(:page_title, t('.title')) %>
<%= ds_paragraph {t('.description')} %>
<%= ds_form_with( model: @landfill_operator, url: @post_path,method: :post) do |f| %>  
  <%= f.ds_text_field(method: :organisation_name) %>
  <%= f.ds_text_field(method: :slft_registration_number) %>
  <%= f.ds_text_field(method: :telephone_number) %>
  <%= f.ds_text_field(method: :email_address) %>
<% end %>
