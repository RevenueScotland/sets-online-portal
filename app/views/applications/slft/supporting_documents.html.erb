<% content_for(:page_title, t('.title')) %>
<%= ds_paragraph {t('.description')} %>
<%= ds_form_with( model: @slft_application, url: @post_path) do |f| %>
  <%= rs_conditional_visibility(visible_value: 'OTHER') do |c| %>
    <% c.with_visibility_control_checkbox_group(builder: f.builder, method: :supporting_document_list, 
             options_list:@slft_application.list_ref_data(:supporting_document_list)) %>
    <% if @slft_application.application_type != 'LO-RA' %>
      <%= f.ds_text_field(method: :supporting_document_other_description) %>
    <% end %>
  <% end%>
<% end %>
