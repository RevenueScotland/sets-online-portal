<% content_for(:page_title, t('.title')) %>
<%= ds_form_with( model: @slft_application, url: @post_path) do |f| %>
  <%= rs_conditional_visibility(visible_value: "Y") do |c| %>
    <% c.with_visibility_control_radio_group(builder: f.builder, method: :naturally_occurring,
      options_list:@slft_application.list_ref_data(:naturally_occurring), alignment: :horizontal) %>
    <%= f.rs_percent(method: :naturally_occurring_percentage) %>
  <% end %>
  <%= f.rs_percent(method: :waste_percentage) %>
  <%= f.rs_percent(method: :added_water_percentage) %>
<% end %>
