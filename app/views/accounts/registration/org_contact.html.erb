<% content_for :page_title, t('.title') %>
<% content_for :page_subtitle, t('.subtitle') %>
<% content_for :page_view, render_page_view(__FILE__) %>

<%= form_for @account, url: @post_path, local: true do |f| %>
  <%= form_errors_for @account %>
  <%= f.fields_for @account.company do |c| %>
    <%= c.text_field :org_email_address %>
    <%= c.text_field :org_telephone %>
    <%= c.text_field :main_rep_name %>
    <% if AccountType.other_organisation?(@account.account_type) %>
      <%= f.text_field :nino %>
    <% end %>
  <% end %>
  <%= f.button 'next', { :name => 'submitted' }  %>
<% end %>  