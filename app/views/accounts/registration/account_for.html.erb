<% content_for :page_title, t('.title') %>
<% content_for :page_subtitle, t('.subtitle') %>
<% content_for :page_description, t('.description') %>
<% content_for :page_view, render_page_view(__FILE__) %>

<%= form_for @account, url: @post_path, local: true do |f| %>
  <%= form_errors_for @account %>
  <%= f.fields_for @account.account_type do |at| %>
    <%= at.collection_radio_buttons_fields(:registration_type, AccountType.list, alignment: 'vertical' ) %>
  <% end %>
  <%= f.button 'next', { :name => 'submitted' }  %>
<% end %>