<% content_for(:navigation_link, :clear_stack)%>
<% content_for(:page_title, t('.title')) %>
<%= ds_paragraph {t('.description')} %>
<%= ds_form_with(model: @user, button_action: :confirm, url: user_process_update_tcs_path) do |f| %>
  <%= f.ds_hidden_field(:username) %>
  <%= ds_link(url: website_text_path('REGTSANDCS', clear_stack: true), name: t('.user_is_signed_ta_cs_link'), target: '_blank') %>
  <%= f.ds_checkbox(method: :user_is_signed_ta_cs) %>
<% end %>
