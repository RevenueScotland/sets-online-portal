<% content_for(:page_title, t('.title')) %>
<%= ds_form_with(model: @user_filter, url: users_path, method: :get, button_action: :find) do |f| %>
  <%= f.ds_text_field(method: :full_name) %>
  <%= f.ds_select(method: :user_is_current, select_options: @user_filter.list_ref_data(:user_is_current), width: 5 ) %>
<% end %>
<%= rs_user_table(users:@users, caption: t('.caption'), id: 'users',
      link: (ds_link(name: t('.new_user'), url: new_user_path) if !account_has_no_service?),
      small_screen: 'scrolling',pagination_collection: @pagination_collection) %>
