
<% content_for :page_view, render_page_view(__FILE__) %>
<% content_for :page_title, t('.title') %>
<% content_for :page_subtitle, t('.subtitle') %>

<%= form_for @claim_payment, url: @post_path, method: :post, local: true do |f| %>
<%= form_errors_for @claim_payment %>
  <%= f.text_field :agent_org_name, { optional:true } %> 
  <%= f.text_field :agent_firstname %> 
  <%= f.text_field :agent_surname %>  
  <%= f.text_field :agent_telephone %>
  <%= f.text_field :agent_email_address %>
  <%= f.text_field :agent_dx_number, { optional:true } %> 
  <%= f.button 'next', { :name => 'submitted' }  %>
<% end %>
