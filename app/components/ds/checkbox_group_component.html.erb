<%= ds_field_wrapper(method: method, one_question: one_question, optional: optional, show_label: show_label, 
                     interpolations: interpolations, type: :field_group, error_link_suffix: first_options_code) do |wrapper| %>
  <%# The Rails function has a value parameter that we tend to call code %>
  <%= builder.collection_check_boxes(method, options_list, code_method, value_method,{}, data_action) do |b| 
      tag.div(b.check_box(class: 'ds_checkbox__input') + b.label(class: 'ds_checkbox__label'),class:"ds_checkbox")
   end %>
<% end %>
