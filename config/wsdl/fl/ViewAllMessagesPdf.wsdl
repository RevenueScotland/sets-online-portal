			<xs:import namespace="http://northgate-is.com/FL/Core"/>
			<xs:element name="ViewAllMessagesPdfRequest">
				<xs:annotation>
					<xs:documentation>This API for attachment to an object i.e. Secure message</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="ParRefno" type="xs:integer"/>
						<xs:element name="Username" type="xs:string"/>
						<xs:element name="OriginalSmsgRefno" type="xs:string"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
