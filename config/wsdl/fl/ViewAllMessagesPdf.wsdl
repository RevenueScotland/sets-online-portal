							<xs:sequence>
								<xs:element name="Attachment" minOccurs="0">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="BinaryData" type="xs:base64Binary"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
		</xs:schema>
