										<xs:simpleType>
											<xs:restriction base="xs:string"/>
										</xs:simpleType>
									</xs:element>
									<xs:element name="ReliefAmount" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Original Relief amount. LBTT_RELIEFS.LBRL_ORIG_AMOUNT</xs:documentation>
