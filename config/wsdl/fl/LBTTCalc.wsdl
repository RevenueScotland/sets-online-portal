													<xs:simpleType>
														<xs:restriction base="xs:decimal"/>
													</xs:simpleType>
												</xs:element>
												<xs:element name="TotalLiability">
													<xs:annotation>
														<xs:documentation>Amount liable for LBTT. LBTT_RETURNS.LBTT_DUE_BEFORE_RELIEFS</xs:documentation>
